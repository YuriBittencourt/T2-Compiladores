def pot(int n, int pot, int res){
    if(pot == 0){
        res = 1;
        return;
    }

    if(pot == 1){
        res = n;
        return;
    }

    if(n == 0){
        return;
    }
    pot = pot - 1;
    res = pow(n,pot);
    res = n * res;
    return;
}